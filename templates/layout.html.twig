<!DOCTYPE html>
<html lang="fr">
    <head>
        <meta charset="UTF-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
        <title>{% block title %}ZOCHI :: {% endblock %}</title>
        <meta name="keywords" content="ASCCI, OMMS, scoutisme, louveteau, eclaireur, cheminot, routier">
        <meta name="description" content="Le scoutisme à la maison, portail d'echange du programme éducatif de l'Association des Scouts Catholiques de Côte d'Ivoire">
        <meta name="author" content="ASCCI-PIT, Delrodie AMOIKON">
        <meta name="Content-Language" content="fr_FR">
        <meta name="Copyright" content="www.scoutascci.org">

        <link rel="canonical" href="http://zochi.scoutascci.com/" />
        <meta property="og:locale" content="fr_FR" />
        <meta property="og:type" content="Item" />
        <meta property="og:title" content="ZOCHI :: Le scoutisme à la maison" />
        <meta property="og:description" content="Le scoutisme à la maison, portail d'echanges du programme éducatif de l'Association des Scouts Catholiques de Côte d'Ivoire" />
        <meta property="og:url" content="http://zochi.scoutascci.com/" />
        <meta property="og:site_name" content="ASCCI" />
        <meta property="article:section" content="Item" />
        <meta property="article:published_time" content="2020-03-30T19:30:59+00:00" />
        <meta property="og:image" content="{{ absolute_url(asset('ressources/images/logo-01.png'))}}" />
        <meta name="twitter:card" content="summary"/>
        <meta name="twitter:description" content="Le scoutisme à la maison, portail d'echange du programme éducatif de l'Association des Scouts Catholiques de Côte d'Ivoire"/>
        <meta name="twitter:title" content="ZOCHI :: Le scoutisme à la maison"/>
        <meta name="twitter:domain" content="ASCCI"/>
        <meta name="twitter:image:src" content="{{ absolute_url(asset('ressources/images/logo-01.png'))}}"/>

        <link rel="icon" type="image/png" href="{{ absolute_url(asset('ressources/images/favicon.ico')) }}">
        {% block stylesheets %}
            <link href="{{ absolute_url(asset('ressources/css/style.css')) }}" rel="stylesheet">
            <link href="{{ absolute_url(asset('ressources/css/responsive.css')) }}" rel="stylesheet">
        {% endblock %}
    </head>
    <body>
        <header class="header">
            <nav class="navbar navbar-expand-lg fixed-top bg-transparent">
                <div class="container">
                    <a class="navbar-brand" href="{{ path('homepage') }}">
                        <img src="{{ asset('assets/images/logo-01.png') }}" alt="logo" class="img-fluid"/>
                    </a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent"
                            aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                        <i class="fa fa-bars"></i>
                    </button>
                    <div class="collapse navbar-collapse h-auto" id="navbarSupportedContent">
                        <ul class="navbar-nav ml-auto menu">
                            <li><a href="{{ path('homepage') }}">Accueil</a></li>
                            {% if is_granted('ROLE_ACN') %}
                                <li><a href="{{ path('projet_index') }}">Projet</a></li>
                            {% endif %}
                            <li><a href="#" class="dropdown-toggle">Activités</a>
                                <ul class="sub-menu">
                                    {{ render(url('branche_menu2')) }}
                                </ul>
                            </li>
                            <li><a href="#">Forum</a></li>
                            <li><a href="#">Tutoriel</a></li>
                            <li><a href="#">FAQ</a></li>
                            <li>
                                <a href="#" class="dropdown-toggle user-avatar">
                                    <img src="{{ asset('assets/images/avatar.png') }}" alt=""> Connexion
                                </a>
                                {% if is_granted('IS_AUTHENTICATED_REMEMBERED') %}
                                    <ul class="sub-menu">
                                        <li><a href="{{ path('utilisateur_show', {'user': app.user.username}) }}">Profile</a></li>
                                        <li><a href="#">Messages</a></li>
                                        <li><a href="{{ path('activite_index') }}">Activités</a></li>
                                        {% if is_granted('ROLE_ADMIN') %}
                                            <li><a href="{{ path('utilisateur_index') }}">Utilisateurs</a></li>
                                        {% endif %}
                                        {% if is_granted('ROLE_SUPER_ADMIN') %}
                                            <li><a href="{{ path('monitoring') }}">Logs</a></li>
                                        {% endif %}
                                        <li><a href="{{ path('app_logout') }}">Deconnexion</a></li>
                                    </ul>
                                {% else%}
                                    <ul class="sub-menu">
                                        <li><a href="{{ path('app_login') }}">Se connecter</a></li>
                                        <li><a href="{{ path('compte_inscription') }}">Créer un compte</a></li>
                                    </ul>
                                {% endif %}
                            </li>
                        </ul>
                    </div>
                </div>
            </nav>
        </header>
        {% block body %}{% endblock %}
        <footer>
            <div class="container">
                <div class="row">
                    <div class="col-lg-5 col-md-12">
                        <div class="footer-left">
                            <ul>
                                <li><a href="#">A propos</a></li>
                                <li><a href="#">Nous contactez</a></li>
                            </ul>
                        </div>
                    </div>
                    <div class="col-lg-7 col-md-12">
                        <div class="footer-right">
                            <ul class="copyright-text">
                                <li><a href="https://scoutascci.org"><img src="{{ asset('assets/images/ascci.png') }}" alt="" width="30"></a></li>
                                <li><div class="ftr-1"><i class="far fa-copyright"></i> 2020 <a href="https://scoutascci.org">Association des Scouts Catholiques de Côte d'Ivoire</a>. Tous droits reservés.</div></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </footer>

        <button class="scroll-top scroll-to-target" data-target="html">
            <img src="{{ absolute_url(asset('ressources/images/top.png')) }}" alt="top">
        </button>
        {% block javascripts %}
            <script src="https://code.jquery.com/jquery-3.4.0.min.js"></script>
            <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
            <script src="{{ absolute_url(asset('ressources/js/wow.min.js')) }}"></script>
            <script type='text/javascript' src='https://platform-api.sharethis.com/js/sharethis.js#property=5e832bf822b9840019ffb3c7&cms=sop' async='async'></script>
            <script src="{{ absolute_url(asset('ressources/js/app.js') )}}"></script>
        {% endblock %}
    </body>
</html>
